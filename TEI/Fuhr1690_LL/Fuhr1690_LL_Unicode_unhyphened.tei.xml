      <author>Heinrihs Fūrmans</author>
      <title>Laimiga Lauliba</title>
      <date>1690</date>
     </sourceDesc>
    </fileDesc>
    <profileDesc>
     <domain type="public">Laicīgie teksti; gadījuma dzeja</domain>
    </profileDesc>
