      <div n="15." type="Verse" id="VD1689_94/Nah_1:15.">
       <lb id="VD1689_94/Nah_0_85"/>
        <div type="Language" lang="Latvian">15. Raugi us teem Kalneem</div>
       <lb id="VD1689_94/Nah_0_86"/>
        <div type="Language" lang="Latvian">tahs Kahjas ta/ kas labbas Wehſtis</div>
       <lb id="VD1689_94/Nah_0_87"/>
        <div type="Language" lang="Latvian">neẜs/ kas Meeru ẜluddina/</div>
       <lb id="VD1689_94/Nah_0_88"/>
