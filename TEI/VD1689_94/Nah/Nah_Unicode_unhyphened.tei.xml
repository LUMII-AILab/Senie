      <div n="5." type="Verse" id="VD1689_94/Nah_2:5.">
       <lb id="VD1689_94/Nah_0_121"/>
        <div type="Language" lang="Latvian">5. Wiņſch peeminnehs ẜawus</div>
       <lb id="VD1689_94/Nah_0_122"/>
        <div type="Language" lang="Latvian">Warrenus/ tomehr tee peedauſiẜees</div>
       <lb id="VD1689_94/Nah_0_123"/>
