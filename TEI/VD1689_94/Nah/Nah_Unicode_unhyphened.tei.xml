      <div n="2." type="Verse" id="VD1689_94/Nah_1:2.">
       <lb id="VD1689_94/Nah_0_12"/>
        <div type="Language" lang="Latvian">2. Weens bahrgs Deews/ un</div>
       <lb id="VD1689_94/Nah_0_13"/>
        <div type="Language" lang="Latvian">weens Atreebejs irr tas KUNgs/</div>
       <lb id="VD1689_94/Nah_0_14"/>
        <div type="Language" lang="Latvian">tas KUNGS irr weens Atreebejs</div>
       <lb id="VD1689_94/Nah_0_15"/>
