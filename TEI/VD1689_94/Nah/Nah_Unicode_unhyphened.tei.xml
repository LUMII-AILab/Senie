      <div n="0." type="Verse" id="VD1689_94/Nah_2:0.">
       <lb id="VD1689_94/Nah_0_94"/>
        <div type="Language" lang="Latvian">0. Nakums ẜluddina prett Niniwi: 2. Kas ẜawas</div>
       <lb id="VD1689_94/Nah_0_95"/>
