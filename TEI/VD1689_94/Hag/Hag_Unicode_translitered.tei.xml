    <fileDesc id="VD1689_94/Hag">
     <sourceDesc>
      <author>Ernsts Gliks</author>
      <title>Tas Praweets Aggajus</title>
      <date>1689_94</date>
     </sourceDesc>
    </fileDesc>
    <profileDesc>
     <domain type="religious">Garīgie teksti; Bībele, Vecā Derība</domain>
    </profileDesc>
