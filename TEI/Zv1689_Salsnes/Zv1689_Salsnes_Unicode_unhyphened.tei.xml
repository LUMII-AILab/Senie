     <sourceDesc>
      <author>nezināms</author>
      <title>[Sālsnesēju amata zvērests]</title>
      <date>1689</date>
     </sourceDesc>
    </fileDesc>
    <profileDesc>
     <chanel>manuscript</chanel>
     <domain type="public">Laicīgie teksti; lietišķie teksti, zvēresti</domain>
