    <fileDesc id="UKG1693">
     <sourceDesc>
      <author>nezināms</author>
      <title>Unterschiedliche Kirchen=Gebete</title>
      <date>1693</date>
     </sourceDesc>
    </fileDesc>
    <profileDesc>
     <domain type="religious">Garīgie teksti; lūgšanas</domain>
    </profileDesc>
   </teiHeader>
   <text>
    <body>
      <pb n="1"/>
      <div type="Paragraph">
       <lb id="UKG1693_1_1"/>
       <div type="Language" lang="German">Unterſchiedliche</div>
       <lb id="UKG1693_1_2"/>
       <div type="Language" lang="German">Kirchen=Gebete /</div>
       <lb id="UKG1693_1_3"/>
       <div type="Language" lang="German">wie ſelbige</div>
       <lb id="UKG1693_1_4"/>
       <div type="Language" lang="German">Bey denen Unteutſchen</div>
       <lb id="UKG1693_1_5"/>
       <div type="Language" lang="German">Gemeinden /</div>
       <lb id="UKG1693_1_6"/>
       <div type="Language" lang="German">Jn und auſſer der Stadt /</div>
       <lb id="UKG1693_1_7"/>
       <div type="Language" lang="German">zu gebrauchen</div>
       <lb id="UKG1693_1_8"/>
       <div type="Language" lang="German">verordnet.</div>
       <lb id="UKG1693_1_9"/>
       <div type="Language" lang="German">RJGA /</div>
       <lb id="UKG1693_1_10"/>
       <div type="Language" lang="German">Bey Georg Matth. Nöllern /</div>
       <lb id="UKG1693_1_11"/>
        <div type="Language" lang="Latvian">1693.</div>
      </div>
      <pb n="2"/>
      <div type="Paragraph">
       <lb id="UKG1693_2_1"/>
        <div type="Language" lang="Latvian">mili un izredzēti iekšan</div>
       <lb id="UKG1693_2_2"/>
        <div type="Language" lang="Latvian">kristo to kungu.</div>
      </div>
      <div type="Paragraph">
       <lb id="UKG1693_2_3"/>
        <div type="Language" lang="Latvian">kad mēs atkal dievā namā</div>
       <lb id="UKG1693_2_4"/>
        <div type="Language" lang="Latvian">un iekšan viņa vārdu /</div>
       <lb id="UKG1693_2_5"/>
        <div type="Language" lang="Latvian">sagājuši esam / dievam mūsam</div>
       <lb id="UKG1693_2_6"/>
        <div type="Language" lang="Latvian">kungam iekšan īstēnas</div>
       <lb id="UKG1693_2_7"/>
        <div type="Language" lang="Latvian">bijašanas kalpot / viņu par</div>
       <lb id="UKG1693_2_8"/>
        <div type="Language" lang="Latvian">savas daudz=kārtigas labdarišanas</div>
       <lb id="UKG1693_2_9"/>
        <div type="Language" lang="Latvian">pateicige slāvet / un par</div>
       <lb id="UKG1693_2_10"/>
        <div type="Language" lang="Latvian">visu / kas mums pie miesas un</div>
       <lb id="UKG1693_2_11"/>
        <div type="Language" lang="Latvian">dvēseles vajāg var būt /</div>
       <lb id="UKG1693_2_12"/>
        <div type="Language" lang="Latvian">piesaukt / bet tas / pirms mēs</div>
       <lb id="UKG1693_2_13"/>
        <div type="Language" lang="Latvian">žēligu piedošanu to grēko</div>
       <lb id="UKG1693_2_14"/>
        <div type="Language" lang="Latvian">dabujuši esam / no mums nē</div>
       <lb id="UKG1693_2_15"/>
        <div type="Language" lang="Latvian">var notikt / tad ikkatris ar man</div>
       <lb id="UKG1693_2_16"/>
        <div type="Language" lang="Latvian">sev priekšan to visevalditaju</div>
       <lb id="UKG1693_2_17"/>
        <div type="Language" lang="Latvian">dievu / katers starp mums šeitan</div>
      </div>
      <pb n="3"/>
      <div type="Paragraph">
       <lb id="UKG1693_3_1"/>
        <div type="Language" lang="Latvian">gir / un visas lietas redz / zemosies</div>
       <lb id="UKG1693_3_2"/>
        <div type="Language" lang="Latvian">/ un ceļos mesdams / no</div>
       <lb id="UKG1693_3_3"/>
        <div type="Language" lang="Latvian">sirds diben ta lūks:</div>
      </div>
      <div type="Paragraph">
       <lb id="UKG1693_3_4"/>
        <div type="Language" lang="Latvian">es nabags grēcigs cilvēks /</div>
       <lb id="UKG1693_3_5"/>
        <div type="Language" lang="Latvian">katers es nē vien iekšan</div>
       <lb id="UKG1693_3_6"/>
        <div type="Language" lang="Latvian">grēkiem ieņemts un piedzimts /</div>
       <lb id="UKG1693_3_7"/>
        <div type="Language" lang="Latvian">bet aridzan / kamēr es esmu dzivojis</div>
       <lb id="UKG1693_3_8"/>
        <div type="Language" lang="Latvian">/ ziname un nieziname /</div>
       <lb id="UKG1693_3_9"/>
        <div type="Language" lang="Latvian">daudz grēkus darijis esmu / sūdzu</div>
       <lb id="UKG1693_3_10"/>
        <div type="Language" lang="Latvian">tev / ock visspēcigs svēts</div>
       <lb id="UKG1693_3_11"/>
        <div type="Language" lang="Latvian">dievs! ka manam milam debeso</div>
       <lb id="UKG1693_3_12"/>
        <div type="Language" lang="Latvian">tēvam / no visas sirds /</div>
       <lb id="UKG1693_3_13"/>
        <div type="Language" lang="Latvian">ka es tev / dievam žēl! ne esmu</div>
       <lb id="UKG1693_3_14"/>
        <div type="Language" lang="Latvian">par visām lietam / ka man</div>
       <lb id="UKG1693_3_15"/>
        <div type="Language" lang="Latvian">pieder / nedz manu tuvaka / ka</div>
       <lb id="UKG1693_3_16"/>
        <div type="Language" lang="Latvian">sev pašu milojis: bet pretieb</div>
       <lb id="UKG1693_3_17"/>
        <div type="Language" lang="Latvian">tev un taviem svētiem bausliem</div>
       <lb id="UKG1693_3_18"/>
        <div type="Language" lang="Latvian">/ ar domašanu / ar vārdiem</div>
       <lb id="UKG1693_3_19"/>
        <div type="Language" lang="Latvian">un darbiem daudz=kārtige grēkojis;</div>
       <lb id="UKG1693_3_20"/>
        <div type="Language" lang="Latvian">un ar to / to laicigu un</div>
       <lb id="UKG1693_3_21"/>
        <div type="Language" lang="Latvian">mūžigu sodibu / jo tu man</div>
      </div>
      <pb n="4"/>
      <div type="Paragraph">
       <lb id="UKG1693_4_1"/>
        <div type="Language" lang="Latvian">pēc tavas bārgās taisnibas</div>
       <lb id="UKG1693_4_2"/>
        <div type="Language" lang="Latvian">nosodiet gribētu / nopelnijs esmu:</div>
       <lb id="UKG1693_4_3"/>
        <div type="Language" lang="Latvian">bet kad tu / miļais dievs un</div>
       <lb id="UKG1693_4_4"/>
        <div type="Language" lang="Latvian">debeso tēvs / solijs esi / ka tu /</div>
       <lb id="UKG1693_4_5"/>
        <div type="Language" lang="Latvian">tava miļa dēla / kristi jēzu</div>
       <lb id="UKG1693_4_6"/>
        <div type="Language" lang="Latvian">mūsa kunga dēļ / visiem nabagiem</div>
       <lb id="UKG1693_4_7"/>
        <div type="Language" lang="Latvian">grēceniekiem žēlastibu</div>
       <lb id="UKG1693_4_8"/>
        <div type="Language" lang="Latvian">parādiet gribi / katri sev no</div>
       <lb id="UKG1693_4_9"/>
        <div type="Language" lang="Latvian">sirds / pie tev atgriežās / un</div>
       <lb id="UKG1693_4_10"/>
        <div type="Language" lang="Latvian">iekšan patiesas ticibas / uz tavu</div>
       <lb id="UKG1693_4_11"/>
        <div type="Language" lang="Latvian">neaptveramu milestibu steidzās</div>
       <lb id="UKG1693_4_12"/>
        <div type="Language" lang="Latvian">/ ja! tiem gribi tu / no tādas</div>
       <lb id="UKG1693_4_13"/>
        <div type="Language" lang="Latvian">tavas milestibas piedot / un</div>
       <lb id="UKG1693_4_14"/>
        <div type="Language" lang="Latvian">tiem nē pielāgādiet / ko tie preti</div>
       <lb id="UKG1693_4_15"/>
        <div type="Language" lang="Latvian">tev grēkojuši gir / tad palaujos</div>
       <lb id="UKG1693_4_16"/>
        <div type="Language" lang="Latvian">es sev droš uz to / un lūdzu tev</div>
       <lb id="UKG1693_4_17"/>
        <div type="Language" lang="Latvian">sirdige / ka tu pēc tādas dārgas</div>
       <lb id="UKG1693_4_18"/>
        <div type="Language" lang="Latvian">solišanas man nabagam</div>
       <lb id="UKG1693_4_19"/>
        <div type="Language" lang="Latvian">grēceniekam visus manus grēkus</div>
       <lb id="UKG1693_4_20"/>
        <div type="Language" lang="Latvian">/ tavam svētam vārdam par</div>
       <lb id="UKG1693_4_21"/>
        <div type="Language" lang="Latvian">godu / gribētu piedot / kristi</div>
       <lb id="UKG1693_4_22"/>
        <div type="Language" lang="Latvian">jēzu dārga nopelniuma un</div>
       <lb id="UKG1693_4_23"/>
        <div type="Language" lang="Latvian">spēcigas aizbildenašanas dēļ /</div>
       <lb id="UKG1693_4_24"/>
        <div type="Language" lang="Latvian">amen.</div>
      </div>
      <pb n="5"/>
      <div type="Paragraph">
       <lb id="UKG1693_5_1"/>
        <div type="Language" lang="Latvian">tas visspēcigs žēligs dievs /</div>
       <lb id="UKG1693_5_2"/>
        <div type="Language" lang="Latvian">katra žēlastiba ikrietu jauna</div>
       <lb id="UKG1693_5_3"/>
        <div type="Language" lang="Latvian">gir / tas gribētu mums nu / pēc savas</div>
       <lb id="UKG1693_5_4"/>
        <div type="Language" lang="Latvian">neaptveramas lēnibas iekšan</div>
       <lb id="UKG1693_5_5"/>
        <div type="Language" lang="Latvian">kristo jezu / visus mūsus</div>
       <lb id="UKG1693_5_6"/>
        <div type="Language" lang="Latvian">grēkus piedot / un žēlastibu</div>
       <lb id="UKG1693_5_7"/>
        <div type="Language" lang="Latvian">dot / ka mēs / caur ta svēta gara</div>
       <lb id="UKG1693_5_8"/>
        <div type="Language" lang="Latvian">spēku <corr>un</corr><sic>nd</sic> paligu / vienu svētu</div>
       <lb id="UKG1693_5_9"/>
        <div type="Language" lang="Latvian">dzievošanu iesākam / un iekšan</div>
       <lb id="UKG1693_5_10"/>
        <div type="Language" lang="Latvian">pastāvigas dieva=bijašanas</div>
       <lb id="UKG1693_5_11"/>
        <div type="Language" lang="Latvian">py tās mūžigas godibas</div>
       <lb id="UKG1693_5_12"/>
        <div type="Language" lang="Latvian">/ varam uzturēti tapt.</div>
       <lb id="UKG1693_5_13"/>
        <div type="Language" lang="Latvian">amen.</div>
      </div>
      <pb n="6"/>
      <div type="Paragraph">
       <lb id="UKG1693_6_1"/>
        <div type="Language" lang="Latvian">o tu dzievs / visvaldigs / un</div>
       <lb id="UKG1693_6_2"/>
        <div type="Language" lang="Latvian">mūžigs dievs / tēvs</div>
       <lb id="UKG1693_6_3"/>
        <div type="Language" lang="Latvian">mūsa kunga jēzu kristi /</div>
       <lb id="UKG1693_6_4"/>
        <div type="Language" lang="Latvian">raditais un <corr>uzturetais</corr><sic>uzturetāis</sic> viso</div>
       <lb id="UKG1693_6_5"/>
        <div type="Language" lang="Latvian">radito lieto / tev ar tavu dēlu</div>
       <lb id="UKG1693_6_6"/>
        <div type="Language" lang="Latvian">un to svētu garu piesaucam</div>
       <lb id="UKG1693_6_7"/>
        <div type="Language" lang="Latvian">mēs sirdige / tu gribētu sev</div>
       <lb id="UKG1693_6_8"/>
        <div type="Language" lang="Latvian">par mums apžēloties / tava</div>
       <lb id="UKG1693_6_9"/>
        <div type="Language" lang="Latvian">miļa dēla dārgas nopelnišanas</div>
       <lb id="UKG1693_6_10"/>
        <div type="Language" lang="Latvian">dēl / katru tu / pēc tavu</div>
       <lb id="UKG1693_6_11"/>
        <div type="Language" lang="Latvian">brinišku padomu / par vienu</div>
       <lb id="UKG1693_6_12"/>
        <div type="Language" lang="Latvian">uperi priekš visiem grēkiem /</div>
       <lb id="UKG1693_6_13"/>
        <div type="Language" lang="Latvian">un videtayju starpan tev un</div>
       <lb id="UKG1693_6_14"/>
        <div type="Language" lang="Latvian">mums iekšan šās pasaules</div>
       <lb id="UKG1693_6_15"/>
        <div type="Language" lang="Latvian">sūtijis esi / caur to / ir tavu taisnu</div>
       <lb id="UKG1693_6_16"/>
        <div type="Language" lang="Latvian">dusmibu par tās pasaules grēkiem</div>
       <lb id="UKG1693_6_17"/>
        <div type="Language" lang="Latvian">/ ir tavu lielu žēlastibu</div>
       <lb id="UKG1693_6_18"/>
        <div type="Language" lang="Latvian">preti mums parādiet. svēti</div>
       <lb id="UKG1693_6_19"/>
        <div type="Language" lang="Latvian">un valdi mūs visus ar tavu</div>
       <lb id="UKG1693_6_20"/>
        <div type="Language" lang="Latvian">svētu garu / sakraji / vadi un</div>
       <lb id="UKG1693_6_21"/>
        <div type="Language" lang="Latvian">iestiprini tavu kristitu draudzibu</div>
       <lb id="UKG1693_6_22"/>
        <div type="Language" lang="Latvian">/ un dod mums tavu žēlasti-</div>
      </div>
      <pb n="7"/>
      <div type="Paragraph">
       <lb id="UKG1693_7_1"/>
        <div type="Language" lang="Latvian">/ ka mēs / pēc tavu</div>
       <lb id="UKG1693_7_2"/>
        <div type="Language" lang="Latvian">svētu vārdu / iekšan patiesas</div>
       <lb id="UKG1693_7_3"/>
        <div type="Language" lang="Latvian">ticibas un svētas dzivošanas</div>
       <lb id="UKG1693_7_4"/>
        <div type="Language" lang="Latvian">/ liedz galam staigajam. pasargi</div>
       <lb id="UKG1693_7_5"/>
        <div type="Language" lang="Latvian">un svēti mūsu mīligu</div>
       <lb id="UKG1693_7_6"/>
        <div type="Language" lang="Latvian">augstu=varenu kēningu un kungu</div>
       <lb id="UKG1693_7_7"/>
        <div type="Language" lang="Latvian">/ katru tu mums aran žēlastibas</div>
       <lb id="UKG1693_7_8"/>
        <div type="Language" lang="Latvian">par vienu galveneku /</div>
       <lb id="UKG1693_7_9"/>
        <div type="Language" lang="Latvian">un glabeiju <corr>iecēlis</corr><sic>iecēxlis</sic> esi. paglabo</div>
       <lb id="UKG1693_7_10"/>
        <div type="Language" lang="Latvian">to d dievs / uz visiem</div>
       <lb id="UKG1693_7_11"/>
        <div type="Language" lang="Latvian">saviem ceļiem / par visu nhelaimu</div>
       <lb id="UKG1693_7_12"/>
        <div type="Language" lang="Latvian">un bēdam / katras viņam</div>
       <lb id="UKG1693_7_13"/>
        <div type="Language" lang="Latvian">ir slepene / ir redzige varetu</div>
       <lb id="UKG1693_7_14"/>
        <div type="Language" lang="Latvian">notikt. iestiprini un vairo</div>
       <lb id="UKG1693_7_15"/>
        <div type="Language" lang="Latvian">viņa spēku pie mēsas un</div>
       <lb id="UKG1693_7_16"/>
        <div type="Language" lang="Latvian">dvēseles / laid uz to dusu tas</div>
       <lb id="UKG1693_7_17"/>
        <div type="Language" lang="Latvian">gars tas gudribas un</div>
       <lb id="UKG1693_7_18"/>
        <div type="Language" lang="Latvian">tās saprašanas / ta padoma</div>
       <lb id="UKG1693_7_19"/>
        <div type="Language" lang="Latvian">un tavas bijašanas. apgaišmo</div>
       <lb id="UKG1693_7_20"/>
        <div type="Language" lang="Latvian">viņa sirdi / ka viņs savu</div>
       <lb id="UKG1693_7_21"/>
        <div type="Language" lang="Latvian">zem un ļaudis it gudrige</div>
       <lb id="UKG1693_7_22"/>
        <div type="Language" lang="Latvian">valda / kristi baznicu un</div>
      </div>
      <pb n="8"/>
      <div type="Paragraph">
       <lb id="UKG1693_8_1"/>
        <div type="Language" lang="Latvian">draudzibu vairo / visai vaidenieko</div>
       <lb id="UKG1693_8_2"/>
        <div type="Language" lang="Latvian">uzbrukšanai un ļauniem</div>
       <lb id="UKG1693_8_3"/>
        <div type="Language" lang="Latvian">padomiem preti stāv / un</div>
       <lb id="UKG1693_8_4"/>
        <div type="Language" lang="Latvian">iekšan labu mieru / tavam</div>
       <lb id="UKG1693_8_5"/>
        <div type="Language" lang="Latvian">svētam vārdam par godu /</div>
       <lb id="UKG1693_8_6"/>
        <div type="Language" lang="Latvian">un saviem ļaudiem par labu /</div>
       <lb id="UKG1693_8_7"/>
        <div type="Language" lang="Latvian">ilge un laimige valda. mūsa</div>
       <lb id="UKG1693_8_8"/>
        <div type="Language" lang="Latvian">augstu=varena kēninga māti /</div>
       <lb id="UKG1693_8_9"/>
        <div type="Language" lang="Latvian">to atraitnu kēninga=gaspažu</div>
       <lb id="UKG1693_8_10"/>
        <div type="Language" lang="Latvian">uztur ar prieku iekš visas kēnigškas</div>
       <lb id="UKG1693_8_11"/>
        <div type="Language" lang="Latvian">labklāšanas un ilgas</div>
       <lb id="UKG1693_8_12"/>
        <div type="Language" lang="Latvian">dzivošanas. mūsa augstu=varenu</div>
       <lb id="UKG1693_8_13"/>
        <div type="Language" lang="Latvian">kēninga=gaspažu</div>
       <lb id="UKG1693_8_14"/>
        <div type="Language" lang="Latvian">gribētu tu iekšan tavas žēlastibas</div>
       <lb id="UKG1693_8_15"/>
        <div type="Language" lang="Latvian">un glabšanas uzturet /</div>
       <lb id="UKG1693_8_16"/>
        <div type="Language" lang="Latvian">ka ta daudz gadus iekš tavas</div>
       <lb id="UKG1693_8_17"/>
        <div type="Language" lang="Latvian">svētibas piedzivot var. to</div>
       <lb id="UKG1693_8_18"/>
        <div type="Language" lang="Latvian">kēningšku dzimtu princ un</div>
       <lb id="UKG1693_8_19"/>
        <div type="Language" lang="Latvian">dzimtas princeses gribētu tu</div>
       <lb id="UKG1693_8_20"/>
        <div type="Language" lang="Latvian">ar visu garigu un mesigu svētibu</div>
       <lb id="UKG1693_8_21"/>
        <div type="Language" lang="Latvian">appuškot / ka te labi uzauga</div>
       <lb id="UKG1693_8_22"/>
        <div type="Language" lang="Latvian">un piejemās iekšan īstenas</div>
       <lb id="UKG1693_8_23"/>
        <div type="Language" lang="Latvian">dievabijašanas un visu krist=</div>
      </div>
      <pb n="9"/>
      <div type="Paragraph">
       <lb id="UKG1693_9_1"/>
        <div type="Language" lang="Latvian">kēnigšku godu / tavam svētam</div>
       <lb id="UKG1693_9_2"/>
        <div type="Language" lang="Latvian">vārdam par slavu un tai</div>
       <lb id="UKG1693_9_3"/>
        <div type="Language" lang="Latvian">tēva=zemei par prieku. apstiprini</div>
       <lb id="UKG1693_9_4"/>
        <div type="Language" lang="Latvian">to visu kēnigšku</div>
       <lb id="UKG1693_9_5"/>
        <div type="Language" lang="Latvian">namu ar ilgu spēku un</div>
       <lb id="UKG1693_9_6"/>
        <div type="Language" lang="Latvian">godu. tos kēninga padomniekus</div>
       <lb id="UKG1693_9_7"/>
        <div type="Language" lang="Latvian">un visus runas=kungus</div>
       <lb id="UKG1693_9_8"/>
        <div type="Language" lang="Latvian">/ ir visus augstus</div>
       <lb id="UKG1693_9_9"/>
        <div type="Language" lang="Latvian">kēninga valdiniekus / seviske</div>
       <lb id="UKG1693_9_10"/>
        <div type="Language" lang="Latvian">mūsa cieniga un stipra</div>
       <lb id="UKG1693_9_11"/>
        <div type="Language" lang="Latvian">kēninga kara=spēku / ta</div>
       <lb id="UKG1693_9_12"/>
        <div type="Language" lang="Latvian">uz jūri / ka uz zemes / ka aridzan</div>
       <lb id="UKG1693_9_13"/>
        <div type="Language" lang="Latvian">visus kēninga sulainus /</div>
       <lb id="UKG1693_9_14"/>
        <div type="Language" lang="Latvian">pasargi par visu / kas ļauns un</div>
       <lb id="UKG1693_9_15"/>
        <div type="Language" lang="Latvian">grēcigs var būt. nogries aridzan</div>
       <lb id="UKG1693_9_16"/>
        <div type="Language" lang="Latvian">/ d žēligs dievs! visu</div>
       <lb id="UKG1693_9_17"/>
        <div type="Language" lang="Latvian">grūtu sodibu un zemes=</div>
       <lb id="UKG1693_9_18"/>
        <div type="Language" lang="Latvian">bēdas / katras mēs / ar grēkiem</div>
       <lb id="UKG1693_9_19"/>
        <div type="Language" lang="Latvian">gan nopelnijuši esam.</div>
       <lb id="UKG1693_9_20"/>
        <div type="Language" lang="Latvian">dod mums pastāvigu veselibu</div>
       <lb id="UKG1693_9_21"/>
        <div type="Language" lang="Latvian">/ svētitu pelnišanu / skaidru</div>
       <lb id="UKG1693_9_22"/>
        <div type="Language" lang="Latvian">milestibu un vienaidibu / ka</div>
       <lb id="UKG1693_9_23"/>
        <div type="Language" lang="Latvian">starp mums viena mieriga un</div>
      </div>
      <pb n="10"/>
      <div type="Paragraph">
       <lb id="UKG1693_10_1"/>
        <div type="Language" lang="Latvian">dieva bijiga valdišana var</div>
       <lb id="UKG1693_10_2"/>
        <div type="Language" lang="Latvian">būt / dod aridzan labu padomu</div>
       <lb id="UKG1693_10_3"/>
        <div type="Language" lang="Latvian">pie visas kristitas apjemšanas</div>
       <lb id="UKG1693_10_4"/>
        <div type="Language" lang="Latvian">un dziru / ka mēs neapgrecige</div>
       <lb id="UKG1693_10_5"/>
        <div type="Language" lang="Latvian">mūso starpa dzivojam un</div>
       <lb id="UKG1693_10_6"/>
        <div type="Language" lang="Latvian">dod mums pēc šās laicigās</div>
       <lb id="UKG1693_10_7"/>
        <div type="Language" lang="Latvian">dzivošanas / to mužigu prieku</div>
       <lb id="UKG1693_10_8"/>
        <div type="Language" lang="Latvian">un svētibu / caur jēzum</div>
       <lb id="UKG1693_10_9"/>
        <div type="Language" lang="Latvian">kristum tavu dēlu /</div>
       <lb id="UKG1693_10_10"/>
        <div type="Language" lang="Latvian">katers ar tev dzivo un valda</div>
       <lb id="UKG1693_10_11"/>
        <div type="Language" lang="Latvian">iekšan vienaidibu</div>
       <lb id="UKG1693_10_12"/>
        <div type="Language" lang="Latvian">ta svēta gara /</div>
       <lb id="UKG1693_10_13"/>
        <div type="Language" lang="Latvian">amen.</div>
      </div>
      <pb n="11"/>
      <div type="Paragraph">
       <lb id="UKG1693_11_1"/>
       <div type="Language" lang="German">Litaney.</div>
       <lb id="UKG1693_11_2"/>
        <div type="Language" lang="Latvian">kungs dievs / apžēlojies par</div>
       <lb id="UKG1693_11_3"/>
        <div type="Language" lang="Latvian">mums /</div>
       <lb id="UKG1693_11_4"/>
        <div type="Language" lang="Latvian">kriste / apžēlojies par</div>
       <lb id="UKG1693_11_5"/>
        <div type="Language" lang="Latvian">mums /</div>
       <lb id="UKG1693_11_6"/>
        <div type="Language" lang="Latvian">kungs dievs apžēlojies par</div>
       <lb id="UKG1693_11_7"/>
        <div type="Language" lang="Latvian">mums.</div>
       <lb id="UKG1693_11_8"/>
        <div type="Language" lang="Latvian">kriste paklausi mums.</div>
       <lb id="UKG1693_11_9"/>
        <div type="Language" lang="Latvian">kungs dievs tēs iekšan debes</div>
       <lb id="UKG1693_11_10"/>
        <div type="Language" lang="Latvian">/</div>
       <lb id="UKG1693_11_11"/>
        <div type="Language" lang="Latvian">apžēlojies par mums.</div>
       <lb id="UKG1693_11_12"/>
        <div type="Language" lang="Latvian">kungs dievs dēls tās pasaules</div>
       <lb id="UKG1693_11_13"/>
        <div type="Language" lang="Latvian">pestitais /</div>
       <lb id="UKG1693_11_14"/>
        <div type="Language" lang="Latvian"><corr>apžēlojies</corr><sic>_ojies</sic> par mums.</div>
       <lb id="UKG1693_11_15"/>
        <div type="Language" lang="Latvian"><corr>dievs</corr><sic>_ievs</sic> svētais gars /</div>
       <lb id="UKG1693_11_16"/>
        <div type="Language" lang="Latvian"><corr>apžēlojies</corr><sic>_es</sic> par mums.</div>
      </div>
      <pb n="12"/>
      <div type="Paragraph">
       <lb id="UKG1693_12_1"/>
        <div type="Language" lang="Latvian">no visiem grēkiem /</div>
       <lb id="UKG1693_12_2"/>
        <div type="Language" lang="Latvian">no visu apalošanu no dieva</div>
       <lb id="UKG1693_12_3"/>
        <div type="Language" lang="Latvian">vārdu /</div>
       <lb id="UKG1693_12_4"/>
        <div type="Language" lang="Latvian">no visu ļaunu /</div>
       <lb id="UKG1693_12_5"/>
        <div type="Language" lang="Latvian">no ta vela viltibu /</div>
       <lb id="UKG1693_12_6"/>
        <div type="Language" lang="Latvian">no ļaunu piepiežu nāvu /</div>
       <lb id="UKG1693_12_7"/>
        <div type="Language" lang="Latvian">no mēra trumem un <corr>bada</corr><sic>bad_</sic></div>
       <lb id="UKG1693_12_8"/>
        <div type="Language" lang="Latvian">laiku /</div>
       <lb id="UKG1693_12_9"/>
        <div type="Language" lang="Latvian">pasargi mums miļais <corr>dievs</corr><sic>diev_</sic></div>
       <lb id="UKG1693_12_10"/>
        <div type="Language" lang="Latvian">un kungs.</div>
       <lb id="UKG1693_12_11"/>
        <div type="Language" lang="Latvian">no karu un asino isliešanu /</div>
       <lb id="UKG1693_12_12"/>
        <div type="Language" lang="Latvian">no soribu un ļaunu <corr>saderešanu</corr><sic>saderesad_šanu</sic></div>
       <lb id="UKG1693_12_13"/>
        <div type="Language" lang="Latvian">/</div>
       <lb id="UKG1693_12_14"/>
        <div type="Language" lang="Latvian">no <corr>uguns</corr><sic>uguns=</sic> un ūdens <corr>grēku</corr><sic>g_</sic></div>
       <lb id="UKG1693_12_15"/>
        <div type="Language" lang="Latvian"><corr>no</corr><sic>_o</sic> krusu un niknu auku</div>
       <lb id="UKG1693_12_16"/>
        <div type="Language" lang="Latvian"><corr>mūžigu</corr><sic>_mūžigu</sic> <corr>nāv</corr><sic>nā_</sic></div>
      </div>
      <pb n="13"/>
      <div type="Paragraph">
       <lb id="UKG1693_13_1"/>
        <div type="Language" lang="Latvian">iekšan mūsu pastaru <corr>stundu</corr><sic>stu_</sic> /</div>
       <lb id="UKG1693_13_2"/>
        <div type="Language" lang="Latvian">iekšan pastara diena sodišanu</div>
       <lb id="UKG1693_13_3"/>
        <div type="Language" lang="Latvian">/</div>
       <lb id="UKG1693_13_4"/>
        <div type="Language" lang="Latvian">paliedz mums</div>
       <div type="Language" lang="Latin">ec.</div>
       <lb id="UKG1693_13_5"/>
        <div type="Language" lang="Latvian">mēs nabagi grēcenieki lūdzam</div>
       <lb id="UKG1693_13_6"/>
        <div type="Language" lang="Latvian">/</div>
       <lb id="UKG1693_13_7"/>
        <div type="Language" lang="Latvian">tu gribētu mūs paklausiet</div>
       <lb id="UKG1693_13_8"/>
        <div type="Language" lang="Latvian">miļais dievs</div>
       <div type="Language" lang="Latin">ec.</div>
       <lb id="UKG1693_13_9"/>
        <div type="Language" lang="Latvian"><corr>un</corr><sic>_nd</sic> tavu svētu kristitu draudzibu</div>
       <lb id="UKG1693_13_10"/>
        <div type="Language" lang="Latvian"><corr>valdi</corr><sic>vald_</sic></div>
      </div>
      <div type="Paragraph">
       <lb id="UKG1693_13_11"/>
        <div type="Language" lang="Latvian">[14.lp]</div>
       <lb id="UKG1693_13_12"/>
        <div type="Language" lang="Latvian">labus ticigus mācitajus tava</div>
       <lb id="UKG1693_13_13"/>
        <div type="Language" lang="Latvian">kristita draudziba sūtiet</div>
       <lb id="UKG1693_13_14"/>
        <div type="Language" lang="Latvian">tavu svētu garu un spēku</div>
       <lb id="UKG1693_13_15"/>
        <div type="Language" lang="Latvian">pie tavu vārdu dot</div>
       <lb id="UKG1693_13_16"/>
        <div type="Language" lang="Latvian">visiem bēdīgiem un <corr>bailīgiem</corr><sic>bailībailī_em</sic></div>
       <lb id="UKG1693_13_17"/>
        <div type="Language" lang="Latvian">paliedzēt un <corr>iepriecināt</corr><sic>ieprieciieprieci_ht</sic></div>
       <lb id="UKG1693_13_18"/>
        <div type="Language" lang="Latvian">.</div>
       <lb id="UKG1693_13_19"/>
        <div type="Language" lang="Latvian">paklausi mūs miļais dievs</div>
       <lb id="UKG1693_13_20"/>
        <div type="Language" lang="Latvian">un kungs.</div>
       <lb id="UKG1693_13_21"/>
        <div type="Language" lang="Latvian">visiem kēningiem un kungiem</div>
       <lb id="UKG1693_13_22"/>
        <div type="Language" lang="Latvian">mieru un vienu prātu</div>
       <lb id="UKG1693_13_23"/>
        <div type="Language" lang="Latvian">.. sve_</div>
      </div>
