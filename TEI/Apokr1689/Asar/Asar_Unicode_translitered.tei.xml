     <sourceDesc>
      <author>Ernsts Gliks</author>
      <title>Asarijus Luhgschana</title>
      <date>1689</date>
     </sourceDesc>
    </fileDesc>
    <profileDesc>
     <domain type="religious">Garīgie teksti; Bībele, apokrifi</domain>
    </profileDesc>
