     <sourceDesc>
      <author>Frīdrihs Kazimirs Urbāns</author>
      <title>Labs Kungs leela Deewa Dahwana</title>
      <date>1791</date>
     </sourceDesc>
    </fileDesc>
    <profileDesc>
     <domain type="public">Laicīgie teksti; gadījuma dzeja</domain>
    </profileDesc>
   </teiHeader>
   <text>
    <body>
      <pb n="1"/>
      <div type="Paragraph">
       <lb id="Urban1791_LKLDD_1_1"/>
        <div type="Language" lang="Latvian">labs kungs</div>
       <lb id="Urban1791_LKLDD_1_2"/>
        <div type="Language" lang="Latvian">liela dieva dāvana.</div>
      </div>
      <div type="Paragraph">
       <lb id="Urban1791_LKLDD_1_3"/>
        <div type="Language" lang="Latvian">gribedami</div>
       <lb id="Urban1791_LKLDD_1_4"/>
        <div type="Language" lang="Latvian">tie lestenes ļaudis</div>
       <lb id="Urban1791_LKLDD_1_5"/>
        <div type="Language" lang="Latvian">savam cienigam</div>
       <lb id="Urban1791_LKLDD_1_6"/>
        <div type="Language" lang="Latvian">tēvam un dzimtskungam</div>
       <lb id="Urban1791_LKLDD_1_7"/>
        <div type="Language" lang="Latvian">fernant fircks</div>
       <lb id="Urban1791_LKLDD_1_8"/>
        <div type="Language" lang="Latvian">kas tos</div>
       <lb id="Urban1791_LKLDD_1_9"/>
        <div type="Language" lang="Latvian">pussimtu gadu</div>
       <lb id="Urban1791_LKLDD_1_10"/>
        <div type="Language" lang="Latvian">labi un lēni valdijis</div>
       <lb id="Urban1791_LKLDD_1_11"/>
        <div type="Language" lang="Latvian">savu pateicigu un priecigu sirdi</div>
       <lb id="Urban1791_LKLDD_1_12"/>
        <div type="Language" lang="Latvian">parādīt</div>
       <lb id="Urban1791_LKLDD_1_13"/>
        <div type="Language" lang="Latvian">viņu mācitajs</div>
       <lb id="Urban1791_LKLDD_1_14"/>
        <div type="Language" lang="Latvian">f. c. urban</div>
       <lb id="Urban1791_LKLDD_1_15"/>
        <div type="Language" lang="Latvian">kas</div>
       <lb id="Urban1791_LKLDD_1_16"/>
        <div type="Language" lang="Latvian">viņu sirds=domas zina tos</div>
       <lb id="Urban1791_LKLDD_1_17"/>
        <div type="Language" lang="Latvian">šinīs vārdos sarakstijis</div>
       <lb id="Urban1791_LKLDD_1_18"/>
        <div type="Language" lang="Latvian">ziedu mēnešā, tanī gadā</div>
       <lb id="Urban1791_LKLDD_1_19"/>
        <div type="Language" lang="Latvian">1791.</div>
      </div>
      <pb n="2"/>
      <div type="Paragraph">
       <lb id="Urban1791_LKLDD_2_1"/>
        <div type="Language" lang="Latvian">sīraka gudr. grām.</div>
       <lb id="Urban1791_LKLDD_2_2"/>
        <div type="Language" lang="Latvian">nod. 10. v. 1.</div>
      </div>
      <div type="Paragraph">
       <lb id="Urban1791_LKLDD_2_3"/>
        <div type="Language" lang="Latvian">kur viens sapratigais valda tur</div>
       <lb id="Urban1791_LKLDD_2_4"/>
        <div type="Language" lang="Latvian">top viss pareizi līdzināts.</div>
      </div>
      <pb n="3"/>
      <div type="Paragraph">
       <lb id="Urban1791_LKLDD_3_1"/>
        <div type="Language" lang="Latvian">dievs šo pasauli gudri</div>
       <lb id="Urban1791_LKLDD_3_2"/>
        <div type="Language" lang="Latvian">valdidams citus licis</div>
       <lb id="Urban1791_LKLDD_3_3"/>
        <div type="Language" lang="Latvian">kalpu kārtā dzīvot, kopdamus</div>
       <lb id="Urban1791_LKLDD_3_4"/>
        <div type="Language" lang="Latvian">to zemi ar savām</div>
       <lb id="Urban1791_LKLDD_3_5"/>
        <div type="Language" lang="Latvian">rokām, jeb sataisidamus citas</div>
       <lb id="Urban1791_LKLDD_3_6"/>
        <div type="Language" lang="Latvian">dāvanas šās jaukas</div>
       <lb id="Urban1791_LKLDD_3_7"/>
        <div type="Language" lang="Latvian">pasaules tiem cilvekiem par</div>
       <lb id="Urban1791_LKLDD_3_8"/>
        <div type="Language" lang="Latvian">pārtikšanu un prieku; citus</div>
       <lb id="Urban1791_LKLDD_3_9"/>
        <div type="Language" lang="Latvian">viņš atkal par kungiem</div>
       <lb id="Urban1791_LKLDD_3_10"/>
        <div type="Language" lang="Latvian">cēlis, valdīt to pulku savu</div>
       <lb id="Urban1791_LKLDD_3_11"/>
        <div type="Language" lang="Latvian">ļaužu ar <corr>taisnibu</corr><sic>taisnibn</sic> un gudribu,</div>
       <lb id="Urban1791_LKLDD_3_12"/>
        <div type="Language" lang="Latvian">ka tie sev pašiem par</div>
       <lb id="Urban1791_LKLDD_3_13"/>
        <div type="Language" lang="Latvian">labu atstātu visu negantibu</div>
      </div>
      <pb n="4"/>
      <div type="Paragraph">
       <lb id="Urban1791_LKLDD_4_1"/>
        <div type="Language" lang="Latvian">un pēc tās taisnibas dzītos</div>
       <lb id="Urban1791_LKLDD_4_2"/>
        <div type="Language" lang="Latvian">visās lietas. labi</div>
       <lb id="Urban1791_LKLDD_4_3"/>
        <div type="Language" lang="Latvian">kungi, kas šo savu augstu</div>
       <lb id="Urban1791_LKLDD_4_4"/>
        <div type="Language" lang="Latvian">amatu pareizi valko patiesi</div>
       <lb id="Urban1791_LKLDD_4_5"/>
        <div type="Language" lang="Latvian">lielu godu un slavu nopelna.</div>
       <lb id="Urban1791_LKLDD_4_6"/>
        <div type="Language" lang="Latvian">tos ka lielu dieva=dāvanu</div>
       <lb id="Urban1791_LKLDD_4_7"/>
        <div type="Language" lang="Latvian">var uzlūkot, un ar</div>
       <lb id="Urban1791_LKLDD_4_8"/>
        <div type="Language" lang="Latvian">sirsnigu pateikšanu būs saņemt.</div>
       <lb id="Urban1791_LKLDD_4_9"/>
        <div type="Language" lang="Latvian">- bet kas atzīst it</div>
       <lb id="Urban1791_LKLDD_4_10"/>
        <div type="Language" lang="Latvian">skaidri, kurš kungs patiesi</div>
       <lb id="Urban1791_LKLDD_4_11"/>
        <div type="Language" lang="Latvian">labs iraid? negudri ļaudis</div>
       <lb id="Urban1791_LKLDD_4_12"/>
        <div type="Language" lang="Latvian">to vien labu kungu teic, kas</div>
       <lb id="Urban1791_LKLDD_4_13"/>
        <div type="Language" lang="Latvian">visadi savus ļaudis savā</div>
       <lb id="Urban1791_LKLDD_4_14"/>
        <div type="Language" lang="Latvian">vaļā palaiž, to pārgalvibu</div>
       <lb id="Urban1791_LKLDD_4_15"/>
        <div type="Language" lang="Latvian">un blēdibu pārskatās,</div>
       <lb id="Urban1791_LKLDD_4_16"/>
        <div type="Language" lang="Latvian">un no visai lielas lēnibas</div>
       <lb id="Urban1791_LKLDD_4_17"/>
        <div type="Language" lang="Latvian">nesoda, kas tos pamet slinkumā,</div>
       <lb id="Urban1791_LKLDD_4_18"/>
        <div type="Language" lang="Latvian">un nedzen darbu strādāt,</div>
       <lb id="Urban1791_LKLDD_4_19"/>
        <div type="Language" lang="Latvian">un pelnīt godigi savu</div>
      </div>
      <pb n="5"/>
      <div type="Paragraph">
       <lb id="Urban1791_LKLDD_5_1"/>
        <div type="Language" lang="Latvian">pārtikšanu, bet kas bez gudra</div>
       <lb id="Urban1791_LKLDD_5_2"/>
        <div type="Language" lang="Latvian">apdoma dod, tam vajadzigam</div>
       <lb id="Urban1791_LKLDD_5_3"/>
        <div type="Language" lang="Latvian">ka tam bezdievigam</div>
       <lb id="Urban1791_LKLDD_5_4"/>
        <div type="Language" lang="Latvian">tēretajam, kas tiem savejiem</div>
       <lb id="Urban1791_LKLDD_5_5"/>
        <div type="Language" lang="Latvian">to maizi atŗauj un visu</div>
       <lb id="Urban1791_LKLDD_5_6"/>
        <div type="Language" lang="Latvian">padomu pa=dzer, izšķērž,</div>
       <lb id="Urban1791_LKLDD_5_7"/>
        <div type="Language" lang="Latvian">un izklīdina. ak nē! tāds</div>
       <lb id="Urban1791_LKLDD_5_8"/>
        <div type="Language" lang="Latvian">kungs ir slikts kungs, tam</div>
       <lb id="Urban1791_LKLDD_5_9"/>
        <div type="Language" lang="Latvian">gŗūta atbildešana jadod</div>
       <lb id="Urban1791_LKLDD_5_10"/>
        <div type="Language" lang="Latvian">priekš ta viss augstaka kunga,</div>
       <lb id="Urban1791_LKLDD_5_11"/>
        <div type="Language" lang="Latvian">ka caur viņa palaižanu</div>
       <lb id="Urban1791_LKLDD_5_12"/>
        <div type="Language" lang="Latvian">daž savā ļaunumā palicis,</div>
       <lb id="Urban1791_LKLDD_5_13"/>
        <div type="Language" lang="Latvian">lielos grēkos, lielā bezkaunibā</div>
       <lb id="Urban1791_LKLDD_5_14"/>
        <div type="Language" lang="Latvian">beidzot kritis; daž ta</div>
       <lb id="Urban1791_LKLDD_5_15"/>
        <div type="Language" lang="Latvian">slinkumā ieradis ka viņš</div>
       <lb id="Urban1791_LKLDD_5_16"/>
        <div type="Language" lang="Latvian">ne sev pašam par labu</div>
       <lb id="Urban1791_LKLDD_5_17"/>
        <div type="Language" lang="Latvian">strādāt vīžo un trūkumu</div>
       <lb id="Urban1791_LKLDD_5_18"/>
        <div type="Language" lang="Latvian">cieš. visus šos, kas</div>
       <lb id="Urban1791_LKLDD_5_19"/>
        <div type="Language" lang="Latvian">savu laicigu un mūžigu lab=</div>
      </div>
      <pb n="6"/>
      <div type="Paragraph">
       <lb id="Urban1791_LKLDD_6_1"/>
        <div type="Language" lang="Latvian">klāšanu pazaudejuši, taps</div>
       <lb id="Urban1791_LKLDD_6_2"/>
        <div type="Language" lang="Latvian">no viņa rokām savā laikā</div>
       <lb id="Urban1791_LKLDD_6_3"/>
        <div type="Language" lang="Latvian">prasiti. - kas īsti labs</div>
       <lb id="Urban1791_LKLDD_6_4"/>
        <div type="Language" lang="Latvian">kungs sakams ir, tas pēc</div>
       <lb id="Urban1791_LKLDD_6_5"/>
        <div type="Language" lang="Latvian">dieva prāta, to tiesu un</div>
       <lb id="Urban1791_LKLDD_6_6"/>
        <div type="Language" lang="Latvian">taisnibu ne vien kopj un mekle</div>
       <lb id="Urban1791_LKLDD_6_7"/>
        <div type="Language" lang="Latvian">visā būšanā, bet pats</div>
       <lb id="Urban1791_LKLDD_6_8"/>
        <div type="Language" lang="Latvian">tas taisnakais visiem ar savu</div>
       <lb id="Urban1791_LKLDD_6_9"/>
        <div type="Language" lang="Latvian">krietnibu priekša staiga. kas</div>
       <lb id="Urban1791_LKLDD_6_10"/>
        <div type="Language" lang="Latvian">bārgi soda to tīšu blēdi,</div>
       <lb id="Urban1791_LKLDD_6_11"/>
        <div type="Language" lang="Latvian">ka viņš pats atgriežās no</div>
       <lb id="Urban1791_LKLDD_6_12"/>
        <div type="Language" lang="Latvian">ļaunā, un citi bīstās, atstāt</div>
       <lb id="Urban1791_LKLDD_6_13"/>
        <div type="Language" lang="Latvian">to ceļu tās taisnibas; un tomēr</div>
       <lb id="Urban1791_LKLDD_6_14"/>
        <div type="Language" lang="Latvian">piedod labprāt tam</div>
       <lb id="Urban1791_LKLDD_6_15"/>
        <div type="Language" lang="Latvian">apalošam kas žēlastibu</div>
       <lb id="Urban1791_LKLDD_6_16"/>
        <div type="Language" lang="Latvian">lūdzās un laboties apsolās;</div>
       <lb id="Urban1791_LKLDD_6_17"/>
        <div type="Language" lang="Latvian">labi atminedams, ka</div>
       <lb id="Urban1791_LKLDD_6_18"/>
        <div type="Language" lang="Latvian">viņš pats ikdienas pret savu</div>
       <lb id="Urban1791_LKLDD_6_19"/>
        <div type="Language" lang="Latvian">debes=kungu apgrēkojās,</div>
      </div>
      <pb n="7"/>
      <div type="Paragraph">
       <lb id="Urban1791_LKLDD_7_1"/>
        <div type="Language" lang="Latvian">kas tam piedod žēligi</div>
       <lb id="Urban1791_LKLDD_7_2"/>
        <div type="Language" lang="Latvian">kad viņš no ļauna atgriežās:</div>
       <lb id="Urban1791_LKLDD_7_3"/>
        <div type="Language" lang="Latvian">kas ne vienu</div>
       <lb id="Urban1791_LKLDD_7_4"/>
        <div type="Language" lang="Latvian">mīļ, jeb godā tur, ne ka</div>
       <lb id="Urban1791_LKLDD_7_5"/>
        <div type="Language" lang="Latvian">to uzticigu un tikušu;</div>
       <lb id="Urban1791_LKLDD_7_6"/>
        <div type="Language" lang="Latvian">bezdievibu un niknumu ienīst</div>
       <lb id="Urban1791_LKLDD_7_7"/>
        <div type="Language" lang="Latvian">visur, kur tas to redz</div>
       <lb id="Urban1791_LKLDD_7_8"/>
        <div type="Language" lang="Latvian">un atron; tas ir labs</div>
       <lb id="Urban1791_LKLDD_7_9"/>
        <div type="Language" lang="Latvian">kungs! -</div>
       <lb id="Urban1791_LKLDD_7_10"/>
        <div type="Language" lang="Latvian">ak kāda žēlastiba tas ir,</div>
       <lb id="Urban1791_LKLDD_7_11"/>
        <div type="Language" lang="Latvian">kad kungi saviem ļaudim trūkumā</div>
       <lb id="Urban1791_LKLDD_7_12"/>
        <div type="Language" lang="Latvian">klātstāv, tos no visa</div>
       <lb id="Urban1791_LKLDD_7_13"/>
        <div type="Language" lang="Latvian">bada sarga, un dārgos</div>
       <lb id="Urban1791_LKLDD_7_14"/>
        <div type="Language" lang="Latvian">laikos uztur. jūs to ne atzīstat,</div>
       <lb id="Urban1791_LKLDD_7_15"/>
        <div type="Language" lang="Latvian">latviešu ļaudis! kāds</div>
       <lb id="Urban1791_LKLDD_7_16"/>
        <div type="Language" lang="Latvian">liels labums jums iekš ta,</div>
       <lb id="Urban1791_LKLDD_7_17"/>
        <div type="Language" lang="Latvian">pār visām araju tautām</div>
       <lb id="Urban1791_LKLDD_7_18"/>
        <div type="Language" lang="Latvian">iraid! - labs kungs ari</div>
       <lb id="Urban1791_LKLDD_7_19"/>
        <div type="Language" lang="Latvian">to labprāt dara, bet citam</div>
      </div>
      <pb n="8"/>
      <div type="Paragraph">
       <lb id="Urban1791_LKLDD_8_1"/>
        <div type="Language" lang="Latvian">nedod savu žēligu paligu,</div>
       <lb id="Urban1791_LKLDD_8_2"/>
        <div type="Language" lang="Latvian">ka tam īsti vajadzigam,</div>
       <lb id="Urban1791_LKLDD_8_3"/>
        <div type="Language" lang="Latvian">kam dievs savu svētibu</div>
       <lb id="Urban1791_LKLDD_8_4"/>
        <div type="Language" lang="Latvian">vienreiz atrāvis, kā viņš</div>
       <lb id="Urban1791_LKLDD_8_5"/>
        <div type="Language" lang="Latvian">atzītu savu niecibu un tas</div>
       <lb id="Urban1791_LKLDD_8_6"/>
        <div type="Language" lang="Latvian">kungs tam var parādīt</div>
       <lb id="Urban1791_LKLDD_8_7"/>
        <div type="Language" lang="Latvian">savu labu sirdi. kas nu caur</div>
       <lb id="Urban1791_LKLDD_8_8"/>
        <div type="Language" lang="Latvian">slinkumu, sliktu strādažanu</div>
       <lb id="Urban1791_LKLDD_8_9"/>
        <div type="Language" lang="Latvian">jeb caur plītešanas un lepnibu</div>
       <lb id="Urban1791_LKLDD_8_10"/>
        <div type="Language" lang="Latvian">visu izgaisinadami, trūkumā</div>
       <lb id="Urban1791_LKLDD_8_11"/>
        <div type="Language" lang="Latvian">nāk, tie lai cieš to</div>
       <lb id="Urban1791_LKLDD_8_12"/>
        <div type="Language" lang="Latvian">algu savas negudribas un</div>
       <lb id="Urban1791_LKLDD_8_13"/>
        <div type="Language" lang="Latvian">blēdibas. - labs kungs</div>
       <lb id="Urban1791_LKLDD_8_14"/>
        <div type="Language" lang="Latvian">beidzot nedzen savus ļaudis</div>
       <lb id="Urban1791_LKLDD_8_15"/>
        <div type="Language" lang="Latvian">pārlieku ar darbiem un liecibām,</div>
       <lb id="Urban1791_LKLDD_8_16"/>
        <div type="Language" lang="Latvian">bet tiem atvēl to vaļu</div>
       <lb id="Urban1791_LKLDD_8_17"/>
        <div type="Language" lang="Latvian">savu dzīvošanu pareizi</div>
       <lb id="Urban1791_LKLDD_8_18"/>
        <div type="Language" lang="Latvian">apkopt un īstā laikā apstrādāt.</div>
       <lb id="Urban1791_LKLDD_8_19"/>
        <div type="Language" lang="Latvian">viņš gāda, kā tiem</div>
      </div>
      <pb n="9"/>
      <div type="Paragraph">
       <lb id="Urban1791_LKLDD_9_1"/>
        <div type="Language" lang="Latvian">ar laiks atliek atpūsties no</div>
       <lb id="Urban1791_LKLDD_9_2"/>
        <div type="Language" lang="Latvian">sava gŗūta darba, savas dzīvibas</div>
       <lb id="Urban1791_LKLDD_9_3"/>
        <div type="Language" lang="Latvian">priecaties šinī pasaulē,</div>
       <lb id="Urban1791_LKLDD_9_4"/>
        <div type="Language" lang="Latvian">pēc savas kārtas; kā</div>
       <lb id="Urban1791_LKLDD_9_5"/>
        <div type="Language" lang="Latvian">vissvairak laiks un vaļa, savu</div>
       <lb id="Urban1791_LKLDD_9_6"/>
        <div type="Language" lang="Latvian">nemirstamu garu kopt,</div>
       <lb id="Urban1791_LKLDD_9_7"/>
        <div type="Language" lang="Latvian">dieva vārdu klausīt, lasīt;</div>
       <lb id="Urban1791_LKLDD_9_8"/>
        <div type="Language" lang="Latvian">mācities, un gudribā</div>
       <lb id="Urban1791_LKLDD_9_9"/>
        <div type="Language" lang="Latvian">ņemties no grāmatām; -</div>
       <lb id="Urban1791_LKLDD_9_10"/>
        <div type="Language" lang="Latvian">bet proti, viņš ari uzrauga,</div>
       <lb id="Urban1791_LKLDD_9_11"/>
        <div type="Language" lang="Latvian">ka tas patiesi notīkst,</div>
       <lb id="Urban1791_LKLDD_9_12"/>
        <div type="Language" lang="Latvian">tiem uzticigus mācitajus</div>
       <lb id="Urban1791_LKLDD_9_13"/>
        <div type="Language" lang="Latvian">dod, tos skubināt uz labumu</div>
       <lb id="Urban1791_LKLDD_9_14"/>
        <div type="Language" lang="Latvian">kā ta vaļa ne top ar</div>
       <lb id="Urban1791_LKLDD_9_15"/>
        <div type="Language" lang="Latvian">slinkošanu un nedarbiem iztēreta.</div>
       <lb id="Urban1791_LKLDD_9_16"/>
        <div type="Language" lang="Latvian">- tāds kungs</div>
       <lb id="Urban1791_LKLDD_9_17"/>
        <div type="Language" lang="Latvian">ir īstens dieva vietnieks</div>
       <lb id="Urban1791_LKLDD_9_18"/>
        <div type="Language" lang="Latvian">šinī pasaulē, viņa laba</div>
       <lb id="Urban1791_LKLDD_9_19"/>
        <div type="Language" lang="Latvian">roka, viens atriebejs par</div>
      </div>
      <pb n="10"/>
      <div type="Paragraph">
       <lb id="Urban1791_LKLDD_10_1"/>
        <div type="Language" lang="Latvian">sodibu tam ļaunu=daritajam,</div>
       <lb id="Urban1791_LKLDD_10_2"/>
        <div type="Language" lang="Latvian">bet visiem taisniem par labu</div>
       <lb id="Urban1791_LKLDD_10_3"/>
        <div type="Language" lang="Latvian">un svētibu ta debesu tēva</div>
       <lb id="Urban1791_LKLDD_10_4"/>
        <div type="Language" lang="Latvian">bagata dāvana.</div>
       <lb id="Urban1791_LKLDD_10_5"/>
        <div type="Language" lang="Latvian">jūs, cienigs žēligs</div>
       <lb id="Urban1791_LKLDD_10_6"/>
        <div type="Language" lang="Latvian">tēvs! tāds taisns</div>
       <lb id="Urban1791_LKLDD_10_7"/>
        <div type="Language" lang="Latvian">un mīligs kungs allaž</div>
       <lb id="Urban1791_LKLDD_10_8"/>
        <div type="Language" lang="Latvian">jūsu ļaudim bijat;</div>
       <lb id="Urban1791_LKLDD_10_9"/>
        <div type="Language" lang="Latvian">saka un apliecina vissi</div>
       <lb id="Urban1791_LKLDD_10_10"/>
        <div type="Language" lang="Latvian">lestenieki ar vienu muti:</div>
       <lb id="Urban1791_LKLDD_10_11"/>
        <div type="Language" lang="Latvian">jūs mūs tik lēni un gudri</div>
       <lb id="Urban1791_LKLDD_10_12"/>
        <div type="Language" lang="Latvian">esiet valdijuši, kā mēs</div>
       <lb id="Urban1791_LKLDD_10_13"/>
        <div type="Language" lang="Latvian">nu caur jūsu žēlastibas</div>
       <lb id="Urban1791_LKLDD_10_14"/>
        <div type="Language" lang="Latvian">kā turigi ļaudis visur zinami</div>
       <lb id="Urban1791_LKLDD_10_15"/>
        <div type="Language" lang="Latvian">esam. mums visiem</div>
       <lb id="Urban1791_LKLDD_10_16"/>
        <div type="Language" lang="Latvian">pārtikšana un pilniba mājās,</div>
       <lb id="Urban1791_LKLDD_10_17"/>
        <div type="Language" lang="Latvian">ka aridzan labs padoms</div>
       <lb id="Urban1791_LKLDD_10_18"/>
        <div type="Language" lang="Latvian">uz to ļaunu dienu, jeb no ka</div>
       <lb id="Urban1791_LKLDD_10_19"/>
        <div type="Language" lang="Latvian">priecaties savā starpā. caur</div>
      </div>
      <pb n="11"/>
      <div type="Paragraph">
       <lb id="Urban1791_LKLDD_11_1"/>
        <div type="Language" lang="Latvian">jūsu tēvišķu valdišanu</div>
       <lb id="Urban1791_LKLDD_11_2"/>
        <div type="Language" lang="Latvian">un apgādažanu, viss</div>
       <lb id="Urban1791_LKLDD_11_3"/>
        <div type="Language" lang="Latvian">tas labums mums nāk, tapēc</div>
       <lb id="Urban1791_LKLDD_11_4"/>
        <div type="Language" lang="Latvian">mēs ari šodien visi</div>
       <lb id="Urban1791_LKLDD_11_5"/>
        <div type="Language" lang="Latvian">ar gavilešanu sanākam,</div>
       <lb id="Urban1791_LKLDD_11_6"/>
        <div type="Language" lang="Latvian">dievam pateikdami, kā viņš</div>
       <lb id="Urban1791_LKLDD_11_7"/>
        <div type="Language" lang="Latvian">mums priekš daudz ļautiņiem</div>
       <lb id="Urban1791_LKLDD_11_8"/>
        <div type="Language" lang="Latvian">izšķīris, šinīs</div>
       <lb id="Urban1791_LKLDD_11_9"/>
        <div type="Language" lang="Latvian">bārgos laikos tādu kungu</div>
       <lb id="Urban1791_LKLDD_11_10"/>
        <div type="Language" lang="Latvian">turēt, un viņa žēlastibu</div>
       <lb id="Urban1791_LKLDD_11_11"/>
        <div type="Language" lang="Latvian">tik daudz gadus baudīt -</div>
       <lb id="Urban1791_LKLDD_11_12"/>
        <div type="Language" lang="Latvian">un vēl, jūs mums paši</div>
       <lb id="Urban1791_LKLDD_11_13"/>
        <div type="Language" lang="Latvian">iekš savu dēla</div>
       <lb id="Urban1791_LKLDD_11_14"/>
        <div type="Language" lang="Latvian">dēlu vienu jaunu un</div>
       <lb id="Urban1791_LKLDD_11_15"/>
        <div type="Language" lang="Latvian">krašnu valditaju audzinajuši,</div>
       <lb id="Urban1791_LKLDD_11_16"/>
        <div type="Language" lang="Latvian">kas jūsu pēdās ies,</div>
       <lb id="Urban1791_LKLDD_11_17"/>
        <div type="Language" lang="Latvian">un ar šo muižu, visu to</div>
       <lb id="Urban1791_LKLDD_11_18"/>
        <div type="Language" lang="Latvian">dieva svētibu dabu, kas</div>
       <lb id="Urban1791_LKLDD_11_19"/>
        <div type="Language" lang="Latvian">iekš tās itin mājo. liela</div>
      </div>
      <pb n="12"/>
      <div type="Paragraph">
       <lb id="Urban1791_LKLDD_12_1"/>
        <div type="Language" lang="Latvian">sirsniga pateiciba jums par</div>
       <lb id="Urban1791_LKLDD_12_2"/>
        <div type="Language" lang="Latvian">visu to, mīļais cienigs</div>
       <lb id="Urban1791_LKLDD_12_3"/>
        <div type="Language" lang="Latvian">kungs! dievs vien jums</div>
       <lb id="Urban1791_LKLDD_12_4"/>
        <div type="Language" lang="Latvian">to var atmaksāt ko jūs</div>
       <lb id="Urban1791_LKLDD_12_5"/>
        <div type="Language" lang="Latvian">mums esiet darijušu! bet</div>
       <lb id="Urban1791_LKLDD_12_6"/>
        <div type="Language" lang="Latvian">raugait, arig’ viņš neva</div>
       <lb id="Urban1791_LKLDD_12_7"/>
        <div type="Language" lang="Latvian">jau paklausijis mūsa dienišķas</div>
       <lb id="Urban1791_LKLDD_12_8"/>
        <div type="Language" lang="Latvian">lūgšanas? dzīvibu</div>
       <lb id="Urban1791_LKLDD_12_9"/>
        <div type="Language" lang="Latvian">un veselibu viņš jums devis</div>
       <lb id="Urban1791_LKLDD_12_10"/>
        <div type="Language" lang="Latvian">jūsu sirmumā. viņa</div>
       <lb id="Urban1791_LKLDD_12_11"/>
        <div type="Language" lang="Latvian">debesu svētiba par jums</div>
       <lb id="Urban1791_LKLDD_12_12"/>
        <div type="Language" lang="Latvian">it redzama bija, kad visi</div>
       <lb id="Urban1791_LKLDD_12_13"/>
        <div type="Language" lang="Latvian">trūkumu ciete jūsu lauki</div>
       <lb id="Urban1791_LKLDD_12_14"/>
        <div type="Language" lang="Latvian">ar augļiem pilditi, ko mēs</div>
       <lb id="Urban1791_LKLDD_12_15"/>
        <div type="Language" lang="Latvian">jūs svētidami bijām apstrādajuši.</div>
       <lb id="Urban1791_LKLDD_12_16"/>
        <div type="Language" lang="Latvian">dažu gadu</div>
       <lb id="Urban1791_LKLDD_12_17"/>
        <div type="Language" lang="Latvian">vēl mūsu lūgšanas jūsu</div>
       <lb id="Urban1791_LKLDD_12_18"/>
        <div type="Language" lang="Latvian">dārgas dienas mums pataupīs,</div>
       <lb id="Urban1791_LKLDD_12_19"/>
        <div type="Language" lang="Latvian">mūsu svētišana</div>
      </div>
      <pb n="13"/>
      <div type="Paragraph">
       <lb id="Urban1791_LKLDD_13_1"/>
        <div type="Language" lang="Latvian">pavadīs jūs ari priekš</div>
       <lb id="Urban1791_LKLDD_13_2"/>
        <div type="Language" lang="Latvian">dieva krēsla! - ak! jūsu</div>
       <lb id="Urban1791_LKLDD_13_3"/>
        <div type="Language" lang="Latvian">gods, jūsu pieminešana</div>
       <lb id="Urban1791_LKLDD_13_4"/>
        <div type="Language" lang="Latvian">paliks mūsu sirdīs</div>
       <lb id="Urban1791_LKLDD_13_5"/>
        <div type="Language" lang="Latvian">līdz mūsu nāves=stundas,</div>
       <lb id="Urban1791_LKLDD_13_6"/>
        <div type="Language" lang="Latvian">un mūsu bērnu=bērni no</div>
       <lb id="Urban1791_LKLDD_13_7"/>
        <div type="Language" lang="Latvian">ta laika teiks un slavēs, kur</div>
       <lb id="Urban1791_LKLDD_13_8"/>
        <div type="Language" lang="Latvian">mēs piecdesmits laimigus</div>
       <lb id="Urban1791_LKLDD_13_9"/>
        <div type="Language" lang="Latvian">gadus piedzīvojuši;</div>
       <lb id="Urban1791_LKLDD_13_10"/>
        <div type="Language" lang="Latvian">dievam godu un pateicibu,</div>
       <lb id="Urban1791_LKLDD_13_11"/>
        <div type="Language" lang="Latvian">dievam, ka tas mums iekš</div>
       <lb id="Urban1791_LKLDD_13_12"/>
        <div type="Language" lang="Latvian">jums savu lielaku dāvanu</div>
       <lb id="Urban1791_LKLDD_13_13"/>
        <div type="Language" lang="Latvian">šinī pasaulē, tādu labu</div>
       <lb id="Urban1791_LKLDD_13_14"/>
        <div type="Language" lang="Latvian">kungu atvēlejis.</div>
      </div>
      <pb n="14"/>
      <div type="Paragraph">
       <lb id="Urban1791_LKLDD_14_1"/>
        <div type="Language" lang="Latvian">viens žēligs kungs, kas labu</div>
       <lb id="Urban1791_LKLDD_14_2"/>
        <div type="Language" lang="Latvian">prātu rāda,</div>
       <lb id="Urban1791_LKLDD_14_3"/>
        <div type="Language" lang="Latvian">ka bērnus visus apmīļo;</div>
       <lb id="Urban1791_LKLDD_14_4"/>
        <div type="Language" lang="Latvian">par saviem ļaudim mīli gāda,</div>
       <lb id="Urban1791_LKLDD_14_5"/>
        <div type="Language" lang="Latvian">un visus visus aplaimo;</div>
       <lb id="Urban1791_LKLDD_14_6"/>
        <div type="Language" lang="Latvian">tas cienijams, un slavejams,</div>
       <lb id="Urban1791_LKLDD_14_7"/>
        <div type="Language" lang="Latvian">ar jauku kroni pušķojams!</div>
      </div>
      <div type="Paragraph">
       <lb id="Urban1791_LKLDD_14_8"/>
        <div type="Language" lang="Latvian">ar prieku mēs tev mūsu</div>
       <lb id="Urban1791_LKLDD_14_9"/>
        <div type="Language" lang="Latvian">mīlam tēvam</div>
       <lb id="Urban1791_LKLDD_14_10"/>
        <div type="Language" lang="Latvian">pussimtus gadus klausijam,</div>
       <lb id="Urban1791_LKLDD_14_11"/>
        <div type="Language" lang="Latvian">par to mēs nu tam augstam dievam</div>
       <lb id="Urban1791_LKLDD_14_12"/>
        <div type="Language" lang="Latvian">ar līgsmu sirdi pateicam</div>
       <lb id="Urban1791_LKLDD_14_13"/>
        <div type="Language" lang="Latvian">tu cienijams, tu slavejams,</div>
       <lb id="Urban1791_LKLDD_14_14"/>
        <div type="Language" lang="Latvian">ar jauku kroni pušķojams!!</div>
      </div>
      <pb n="15"/>
      <div type="Paragraph">
       <lb id="Urban1791_LKLDD_15_1"/>
        <div type="Language" lang="Latvian">siņa.</div>
      </div>
      <div type="Paragraph">
       <lb id="Urban1791_LKLDD_15_2"/>
        <div type="Language" lang="Latvian">šo gavilešanas grāmatu tas vecakais</div>
       <lb id="Urban1791_LKLDD_15_3"/>
        <div type="Language" lang="Latvian">vīrs no ta pagasta, tam cienigam</div>
       <lb id="Urban1791_LKLDD_15_4"/>
        <div type="Language" lang="Latvian">kungam nodeve, par piemiņu</div>
       <lb id="Urban1791_LKLDD_15_5"/>
        <div type="Language" lang="Latvian">šīs priecas dienas; bet to ziņgi divpadesmits</div>
       <lb id="Urban1791_LKLDD_15_6"/>
        <div type="Language" lang="Latvian">meitas, to ar vārpu kroni</div>
       <lb id="Urban1791_LKLDD_15_7"/>
        <div type="Language" lang="Latvian">pušķodami, dziedaja.</div>
      </div>
    </body>
